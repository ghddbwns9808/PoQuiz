<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.pokemon.mapper.PokemonMapper">
    <select id="selectPokemon" parameterType="string" resultType="Pokemon">
        SELECT * FROM pokemon WHERE pokemon_number = #{pokemonNumber}
    </select>

</mapper>